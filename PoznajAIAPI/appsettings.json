{
  "JwtSettings": {
    "Issuer": "${JWT_ISSUER}",
    "Audicence": "${JWT_AUDIENCE}",
    "Key": "${JWT_KEY}"
  },
  "ConnectionStrings": {
    "DefaultConnection": "${DEFAULT_DATABASE_CONNECTION_STRING}",
    "HangfireConnection": "${HANGFIRE_DATABASE_CONNECTION_STRING}"
  },
  "Hangfire": {
    "AccessId": "${HANGFIRE_ACCESS_ID}"
  },
  "VideoConversion": {
    "VideoBitrate": 1000,
    "AudioBitrate": 128,
    "Resolution": "640x480",
    "OutputDirectory": "Uploads/Videos"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
